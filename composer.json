{
  "name": "kod3r/log",
  "type": "symfony-bundle",
  "description": "Symfony2 logger bundle saved in database using Doctrine2 ORM",
  "keywords": [
    "symfony2",
    "monolog",
    "logger",
    "doctrine",
    "database"
  ],
  "homepage": "https://github.com/webkod3r/Kod3rLogBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Pablo Molina",
      "email": "web.kod3r@gmail.com",
      "homepage": "http://webkod3r.hol.es",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.3.9",
    "symfony/framework-bundle": "~2.3",
    "doctrine/orm": ">=2.1",
    "monolog/monolog": ">=1.12"
  },
  "require-dev":{
    "doctrine/doctrine-bundle": "~1.3"
  },
  "autoload": {
    "psr-0": { "Kod3r\\LogBundle\\": "" }
  },
  "target-dir": "Kod3r/LogBundle"
}

